{
  "name": "validatable",
  "version": "0.21.0",
  "description": "A library for synchronous and asynchronous input validation.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "ava": {
    "files": [
      "./tests/*.js",
      "./tests/**/*.js"
    ],
    "concurrency": 4,
    "failFast": true
  },
  "scripts": {
    "clean": "rm -Rf ./dist",
    "build": "npm run clean; tsc",
    "prepublish": "npm run build",
    "test": "npm run build && ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xpepermint/validatablejs.git"
  },
  "bugs": {
    "url": "https://github.com/xpepermint/validatablejs/issues"
  },
  "homepage": "https://github.com/xpepermint/validatablejs#readme",
  "keywords": [
    "validatable",
    "validation",
    "validating",
    "validate",
    "valid",
    "isvalid",
    "check",
    "checking",
    "block"
  ],
  "author": "Kristijan Sedlak (Xpepermint)",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.17.0",
    "typescript": "^2.1.4"
  },
  "dependencies": {
    "lodash.merge": "^4.6.0",
    "typeable": "^0.21.0"
  }
}
